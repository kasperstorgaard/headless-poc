{
  "name": "server",
  "version": "0.1.0",
  "description": "Basic express server side rendered application.",
  "main": "dist/index.js",
  "scripts": {
    "dev:build": "tsc src/index.ts --outFile dist/index.js --watch",
    "dev:start": "nodemon dist/index.js",
    "dev": "npm-run-all --parallel dev:build dev:start",
    "prod:build": "tsc src/index.ts --outFile dist/index.js",
    "prod:start": "node dist/index.js",
    "start": "npm-run-all --sequential prod:build prod:start",
    "test": "echo \"Error: no test specified\" && exit 1",
    "types": "node ./update-types.js"
  },
  "author": "kasperstorgaard",
  "license": "ISC",
  "devDependencies": {
    "kentico-cloud-model-generator-utility": "^1.2.0",
    "nodemon": "^1.18.7",
    "npm-run-all": "^4.1.5",
    "ts-node": "^7.0.1",
    "typescript": "^3.2.1"
  },
  "dependencies": {
    "kentico-cloud-delivery": "^5.1.1",
    "rxjs": "^6.3.3"
  }
}
